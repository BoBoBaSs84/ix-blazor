<ix-button @onclick="Clicked" class="@Class"
id="@Id"
disabled="@Disabled" 
ghost="@Ghost" 
outline="@Outline" 
selected="@Selected" 
type="@Type" 
variant="@Variant"
data-toggle="@DataToggle">
    @if(ButtonIcon != null)
    {
        <ix-icon name="@ButtonIcon.Name"></ix-icon>
    }
    @ChildContent
</ix-button>
