@using Microsoft.JSInterop;
@using SiemensIXBlazor.Enums.DatePicker;
@using SiemensIXBlazor.Helpers;
@namespace SiemensIXBlazor.Components
@inject IJSRuntime JSRuntime
@inherits IXBaseComponent

<ix-date-picker @attributes="UserAttributes"
                id="@Id"
                from="@From"
                to="@To"
                corners="@(EnumParser<DatePickerCorners>.ParseEnumToString(Corners))"
                format="@Format"
                max-date="@MaxDate"
                min-date="@MinDate"
                range="@Range"
                locale="@Locale"
                i18n-done="@I18nDone"
                week-start-index="@WeekStartIndex"
                style="@Style"
                class="@Class"></ix-date-picker>
