@using Microsoft.JSInterop;
@inject IJSRuntime JSRuntime
@namespace SiemensIXBlazor.Components
@inherits IXBaseComponent

<ix-time-picker
@attributes="@UserAttributes"
id="@Id"
corners="@Corners" 
format="@Format" 
show-hour="@ShowHour" 
show-minutes="@ShowMinutes" 
show-seconds="@ShowSeconds" 
show-time-reference="@ShowTimeReference" 
text-select-time="@TextSelectTime" 
time="@Time"
style="display: block; width: 20rem; @Style"
class="@Class"></ix-time-picker>
